    "turbo": "^2.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "yarn@1.22.21",
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@types/react": "~19.0.0",
    "@types/react-dom": "~19.0.0"
